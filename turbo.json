{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^tsc"],
      "outputs": ["dist/**"],
      "cache": true,
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "tsconfig.build.json", "tsconfig.json"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"],
      "cache": true
    },
    "lint": {
      "outputs": []
    },
    "tsc": {
      "dependsOn": ["^tsc"],
      "outputs": []
    }
  }
}
