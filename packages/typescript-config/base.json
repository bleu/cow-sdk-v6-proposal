{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2020",
    "module": "commonjs",
    "lib": ["es2020", "dom"],
    "baseUrl": ".",
    "paths": {
      "@cowprotocol/abi": ["../../pkg/cow-abi/src"],
      "@cowprotocol/abi/*": ["../../pkg/cow-abi/src/*"],
      "@cowprotocol/common": ["../../pkg/cow-common/src"],
      "@cowprotocol/common/*": ["../../pkg/cow-common/src/*"],
      "@cowprotocol/composable": ["../../pkg/cow-composable/src"],
      "@cowprotocol/composable/*": ["../../pkg/cow-composable/src/*"],
      "@cowprotocol/config": ["../../pkg/cow-config/src"],
      "@cowprotocol/config/*": ["../../pkg/cow-config/src/*"],
      "@cowprotocol/cow-shed": ["../../pkg/cow-shed/src"],
      "@cowprotocol/cow-shed/*": ["../../pkg/cow-shed/src/*"],
      "@cowprotocol/order-book": ["../../pkg/cow-order-book/src"],
      "@cowprotocol/order-book/*": ["../../pkg/cow-order-book/src/*"],
      "@cowprotocol/order-signing": ["../../pkg/cow-order-signing/src"],
      "@cowprotocol/order-signing/*": ["../../pkg/cow-order-signing/src/*"],
      "@cowprotocol/sdk": ["../../pkg/cow-sdk/src"],
      "@cowprotocol/sdk/*": ["../../pkg/cow-sdk/src/*"],
      "@cowprotocol/subgraph": ["../../pkg/cow-subgraph/src"],
      "@cowprotocol/subgraph/*": ["../../pkg/cow-subgraph/src/*"],
      "@cowprotocol/trading": ["../../pkg/cow-trading/src"],
      "@cowprotocol/trading/*": ["../../pkg/cow-trading/src/*"]
    },
    "typeRoots": ["../../node_modules/@types"],
    "types": ["jest", "node"]
  },
  "exclude": ["node_modules", "dist", "**/*.spec.ts", "**/*.test.ts", "**/__mocks__/**"]
}
