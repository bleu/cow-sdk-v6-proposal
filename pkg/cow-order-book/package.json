{
  "name": "@cowprotocol/order-book",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "src/__generated__/**"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json --outDir dist/",
    "tsc": "tsc --noEmit",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "lint": "eslint . --ext .ts,.tsx",
    "swagger:codegen": " openapi --input https://raw.githubusercontent.com/cowprotocol/services/v2.291.0/crates/orderbook/openapi.yml --output src/__generated__ --exportServices false --exportCore false"
  },
  "dependencies": {
    "@cowprotocol/contracts": "^1.7.0",
    "exponential-backoff": "^3.1.1",
    "limiter": "^3.0.0",
    "@cowprotocol/common": "workspace:^",
    "@cowprotocol/config": "workspace:^"
  },
  "devDependencies": {
    "openapi-typescript-codegen": "^0.23.0",
    "@vercel/style-guide": "^5.2.0",
    "eslint-config-turbo": "^2.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "typescript": "5.1.6",
    "@types/jest": "^29.5.14",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^16.0.0"
  }
}
