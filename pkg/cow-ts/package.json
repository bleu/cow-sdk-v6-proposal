{
  "name": "@cowprotocol/cow-ts",
  "version": "0.0.0",
  "private": true,
  "main": "tist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json --outDir dist/",
    "tsc": "tsc --noEmit",
