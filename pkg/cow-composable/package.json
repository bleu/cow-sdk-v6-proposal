{
  "name": "@cowprotocol/composable",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json --outDir dist/",
    "tsc": "tsc --noEmit",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests --ci --coverage --coverageReporters=json --reporters=default --reporters=jest-junit --json --outputFile=jest.results.json"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-turbo": "^2.3.4",
    "eslint-plugin-only-warn": "^1.1.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "@cowprotocol/contracts": "1.6.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@openzeppelin/merkle-tree": "^1.0.7",
    "ethers": "^5.7.2",
    "@cowprotocol/order-book": "workspace:^",
    "@cowprotocol/cow-ts": "workspace:^",
    "@cowprotocol/sdk-ethers-v5": "workspace:^"
  }
