{
  "name": "@cowprotocol/sdk-ethers-v5",
  "version": "0.0.0",
  "private": true,
  "files": [
    "dist/**"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "typechain:codegen": "typechain --target ethers-v5 --out-dir ./src/__generated__ '../cow-abi/src/abis/*.json'",
    "lint": "eslint . --ext .ts,.tsx",
    "build": "tsc -p tsconfig.build.json --outDir dist/",
    "tsc": "tsc --noEmit",
    "test": "jest --config jest.config.js",
    "test:ci": "jest --ci --coverage --passWithNoTests --reporters=default --reporters=jest-junit --json --outputFile=jest.results.json"
  },
  "dependencies": {
    "@cowprotocol/contracts": "^1.7.0"
  },
  "peerDependencies": {
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/style-guide": "^5.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.0.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "ethers": "^5.7.2",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.2.5",
    "typechain": "^8.2.0",
    "typescript": "5.1.6"
  }
}
